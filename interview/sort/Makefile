all: BubbleSort SelectSort InsertSort ShellSort QuickSort

clean:
	 rm -f BubbleSort
	 rm -f SelectSort
	 rm -f InsertSort
	 rm -f ShellSort
	 rm -f QuickSort

BubbleSort:
	g++ BubbleSort.cpp -o BubbleSort -std=c++11
SelectSort:
	g++ SelectSort.cpp -o SelectSort -std=c++11
InsertSort:
	g++ InsertSort.cpp -o InsertSort -std=c++11
ShellSort:
	g++ ShellSort.cpp -o ShellSort -std=c++11
QuickSort:
	g++ QuickSort.cpp -o QuickSort -std=c++11
